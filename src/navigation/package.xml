<?xml version="1.0"?>
<package>
  <name>navigation</name>
  <version>0.0.0</version>
  <description>The navigation package</description>

  <maintainer email="cbandera@posteo.de">Claudio Bandera</maintainer>
  <maintainer email="Pio_Orzechowski@gmx.com">Piotr Orzechowski</maintainer>

  <url>https://wiki.kitcar-team.de/doku.php?id=konzept_und_umsetzung:navigation:start</url>

  <!-- One license tag required, multiple allowed, one license per tag -->
  <!-- Commonly used license strings: -->
  <!--   BSD, MIT, Boost Software License, GPLv2, GPLv3, LGPLv2.1, LGPLv3 -->
